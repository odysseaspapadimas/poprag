{
  "name": "poprag",
  "description": "",
  "author": "Odysseas Papadimas <odysseas.papadimas9@gmail.com>",
  "version": "0.7.1",
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "protobufjs"
    ]
  },
  "keywords": [
    "better-auth",
    "biome",
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply poprag --remote",
    "db:push": "drizzle-kit push",
    "db:rebuild-fts": "wrangler d1 execute poprag --remote --file scripts/rebuild-fts.sql",
    "db:seed": "npx tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "start": "vite start",
    "typecheck": "npx tsc --noEmit "
  },
