{
  "name": "kolm-start",
  "description": "TanStack Start starter with tRPC, Drizzle ORM, better-auth and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "version": "0.7.1",
  "type": "module",
  "keywords": [
    "better-auth",
    "biome",
    "drizzle-orm",
    "tailwindcss",
    "tanstack-start",
    "tRPC",
    "typescript"
  ],
  "scripts": {
    "build": "vite build && tsc --noEmit",
    "check": "npx @biomejs/biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx src/db/seed.ts",
    "db:seed-models": "npx tsx src/db/seed-models.ts > seed-models.sql && wrangler d1 execute poprag --remote --file seed-models.sql && rm seed-models.sql",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "npx @biomejs/biome format --write .",
    "lint": "npx @biomejs/biome lint --write .",
    "start": "vite start",
    "typecheck": "npx tsc --noEmit "
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.65",
    "@ai-sdk/react": "^2.0.93",
    "@hookform/resolvers": "^5.2.2",
    "@langchain/textsplitters": "^1.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-devtools": "^0.8.0",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.4",
    "@tanstack/react-router-devtools": "^1.134.4",
    "@tanstack/react-router-ssr-query": "^1.134.4",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.134.7",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "ai": "^5.0.93",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.553.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "superjson": "^2.2.5",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@cloudflare/vite-plugin": "^1.14.1",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "drizzle-kit": "^0.31.6",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.47.0"
  }
}
